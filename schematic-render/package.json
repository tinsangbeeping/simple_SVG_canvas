{
  "name": "schematic-render",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "kicad:summary": "tsx src/importers/kicad/cli-summary.ts",
    "kicad:ir": "tsx src/importers/kicad/cli-ir.ts",
    "kicad:symboldef": "tsx src/importers/kicad/cli-symboldef.ts",
    "kicad:import": "tsx src/importers/kicad/cli-import.ts",
    "kicad:check": "tsx src/importers/kicad/cli-import.ts src/importers/kicad/fixtures/min.kicad_sym 2>/dev/null | diff - src/importers/kicad/fixtures/min.kicad_sym.expected.json && tsx src/importers/kicad/cli-symboldef.ts src/importers/kicad/fixtures/min.kicad_sym 2>/dev/null | diff - src/importers/kicad/fixtures/min.expected.symboldef.json"
  },
  "dependencies": {
    "elkjs": "^0.11.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "tsx": "^4.21.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4"
  }
}
